{
    "name": "adrianmejias/laravel-routing-numbers",
    "description": "Laravel API for ACH/NACHA Bank Routing Numbers.",
    "keywords": [
        "routing-numbers",
        "rtn",
        "wrapper",
        "api",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://adrianmejias.com",
    "support": {
        "issues": "https://github.com/adrianmejias/laravel-routing-numbers/issues",
        "forum": "https://twitter.com/adrianmejias",
        "source": "https://github.com/adrianmejias/laravel-routing-numbers"
    },
    "autoload": {
        "psr-4": {
            "AdrianMejias\\RoutingNumbers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdrianMejias\\RoutingNumbers\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adrian Mejias",
            "email": "adrianmejias@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2|^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "illuminate/support": "^9.17"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "guzzlehttp/guzzle": "^7.4",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.5",
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.7",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AdrianMejias\\RoutingNumbers\\Providers\\RoutingNumbersServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": [
            "@test-unit",
            "@test-feature"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test-unit": "vendor/bin/pest --testsuite Unit",
        "test-feature": "vendor/bin/pest --testsuite Feature",
        "test-coverage": "vendor/bin/pest --coverage-clover clover.xml",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
